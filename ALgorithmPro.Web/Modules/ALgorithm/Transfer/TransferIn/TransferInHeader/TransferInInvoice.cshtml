@using ALgorithmPro.Reports
@using DevExpress.AspNetCore

<link href="~/Content/devexpress/css/dx-reporting-skeleton-screen.css" rel="stylesheet" />
<script src="~/Scripts/devexpress/js/knockout.js"></script>
<link href="~/Content/devexpress/css/viewer.part.bundle.css" rel="stylesheet" />
<script src="~/Scripts/devexpress/js/viewer.part.bundle.js"></script>
<link href="~/Content/devextreme/vendor.css" rel="stylesheet" />


@{
    double DetailID = 0;
    if (!AS.IsNullValue(ViewBag.DetailID))
    {
        DetailID = AS.ToDouble(ViewBag.DetailID);
    }

}

@Html.DevExpress().WebDocumentViewer("DocumentViewer").Height("600px").Bind(new TransferInInvoice(DetailID));

