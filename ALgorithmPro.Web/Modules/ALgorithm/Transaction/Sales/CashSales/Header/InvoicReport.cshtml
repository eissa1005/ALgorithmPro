@using ALgorithmPro.Reports
@using DevExpress.AspNetCore

<link href="~/Scripts/devexpress/css/dx-reporting-skeleton-screen.css" rel="stylesheet" />
<script src="~/Scripts/devexpress/knockout/build/js/knockout-latest.js"></script>
<link href="~/Scripts/devexpress/css/viewer.part.bundle.css" rel="stylesheet" />
<script src="~/Scripts/devexpress/js/viewer.part.bundle.js"></script>

@{
    double HeaderID = 0;
    if (!AS.IsNullValue(ViewBag.HeaderID))
    {
        HeaderID = AS.ToDouble(ViewBag.HeaderID);
    }
}

@Html.DevExpress().WebDocumentViewer("DocumentViewer").Height("600px").Bind(new CashSalesInvoice(HeaderID));



